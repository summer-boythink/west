{
    "name": "summer/west",
    "autoload": {
        "psr-4": {
            "Summer\\West\\": "src/",
            "Tests\\": "tests/"
        },
        "classmap": [
            "src/Ast/",
            "src/Object/"
        ]
    },
    "authors": [
        {
            "name": "summer-boythink",
            "email": "2071792205@qq.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "pestphp/pest": "*",
        "laravel/pint": "^1.18"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "start": "@php bin/main.php",
        "dump-autoload": "composer dump-autoload",
        "fmt": "./vendor/bin/pint",
        "test": "./vendor/bin/pest"
    }
}